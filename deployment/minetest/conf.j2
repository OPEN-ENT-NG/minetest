{
    "name": "{{minetestVersion}}",
    "config": {
        "main" : "fr.openent.minetest.Minetest",
        "port" : 8055,
        "app-name" : "Minetest",
        "app-address" : "/minetest",
        "app-icon" : "Minetest-large",
        "host": "https://{{ host }}",
        "ssl" : true,
        {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
        "auto-redeploy": false,
        "userbook-host": "https://{{ host }}",
        "integration-mode" : "HTTP",
        "app-registry.port" : 8012,
        "mode" : "prod",
        "entcore.port" : 8009,
        "minetest-download" : "{{ minetestDownload | default('https://github.com/minetest/minetest/releases/download/5.5.0/minetest-5.5.0-win64.zip') }}",
        "minetest-server" : "{{ minetestServer | default('') }}",
        "minetest-python-server-port" : "{{ minetestPythonServerPort | default('443') }}",
        "minetest-port-range" : "{{ minetestPortRange | default('30000-30100') }}",
        "minetest-link": "{{ minetestLink | default('https://www.minetest.net/downloads/') }}",
        "minetest-wiki": "{{ minetestWiki | default('https://wiki.minetest.net/Privileges/fr') }}",
		"minetest-messaging": "{{ minetestMessaging | default('') }}",
        "publicConf": {
            "xiti": {
                "LIBELLE_SERVICE": {
                    "default": "RESSOURCE_ACCOMPAGNEMENT_ENTRAINEMENT"
                },
                "OUTIL": "MINETEST"
            }
        }
    }
}